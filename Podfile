# platform :ios, '9.0'

target '惠豆商城' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for 惠豆商城
  # 网络请求
  pod 'AFNetworking'
  
  # 图片请求
  pod 'SDWebImage', '~> 5.0.0'
  
  # 下拉刷新
  pod 'MJRefresh'
  
  # 提醒窗,配合MBProgressHUD (MJ)分类更简单
  pod 'MBProgressHUD'
  
  # 相对布局
  pod 'Masonry'
  pod 'SDWebImage'
  pod 'YYModel'
  pod 'YYCache'
  pod 'MJExtension'
  pod 'WechatOpenSDK'
  
  target '惠豆商城Tests' do
    inherit! :search_paths
    # Pods for testing
  end
  
  target '惠豆商城UITests' do
    inherit! :search_paths
    # Pods for testing
  end
  
end
